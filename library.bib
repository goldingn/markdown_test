title = {{A continental risk map for malaria mosquito (Diptera : Culicidae) vectors in Europe}},
url = {<Go to ISI>://000178372000010 file:///E:/Documents and Settings/Administrator/Desktop/Papers/Introduced disease risk/Kuhn 2002.pdf},
