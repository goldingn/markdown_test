file = {:C$\backslash$:/Users/Nick/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Adam, Mutanga, Rugege - 2009 - Multispectral and hyperspectral remote sensing for identification and mapping of wetland vegetation a review(2).pdf:pdf},
issn = {0923-4861},
journal = {Wetlands Ecology and Management},
