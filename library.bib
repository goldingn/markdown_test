file = {:C$\backslash$:/Users/Nick/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Koenraadt, Githeko, Takken - 2004 - The effects of rainfall and evapotranspiration on the temporal dynamics of Anopheles gambiae s.s. and Anopheles arabiensis in a Kenyan village.pdf:pdf},
issn = {0001-706X},
journal = {Acta Tropica},
keywords = {Animals,Anopheles,Environment,Female,Humans,Humidity,Kenya,Kenya: epidemiology,Larva,Malaria,Malaria: epidemiology,Plant Transpiration,Population Dynamics,Rain,Rural Population,Time Factors},
