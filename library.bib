issn = {1742-5662},
journal = {Journal of the Royal Society, Interface},
keywords = {Animals,Bluetongue,Bluetongue: epidemiology,Ceratopogonidae,Ceratopogonidae: virology,Climate Change,Disease Outbreaks,Europe,Europe: epidemiology,Insect Vectors,Insect Vectors: virology,Models,Rain,Risk Assessment,Ruminants,Ruminants: virology,Temperature,Theoretical,Uncertainty},
