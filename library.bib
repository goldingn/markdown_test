keywords = {Animals,Argentina,Carbon Dioxide,Carbon Dioxide: metabolism,Chagas Disease,Chagas Disease: prevention \& control,Chickens,Climate,Housing, Animal,Humans,Insect Control,Insect Control: methods,Insect Vectors,Insect Vectors: physiology,Saccharomyces cerevisiae,Saccharomyces cerevisiae: metabolism,Triatoma,Triatoma: physiology},
month = jul,
