journal = {Medical and Veterinary Entomology},
keywords = {Alphavirus Infections,Alphavirus Infections: transmission,Alphavirus Infections: virology,Animals,Biodiversity,Cluster Analysis,Culicidae,Culicidae: growth \& development,Culicidae: virology,DNA,Ecosystem,Fresh Water,Humans,Insect Vectors,Insect Vectors: growth \& development,Insect Vectors: virology,RNA,Reverse Transcriptase Polymerase Chain Reaction,Sequence Analysis,Sindbis Virus,Sindbis Virus: growth \& development,Sweden,Viral,Viral: chemistry,Viral: genetics},
