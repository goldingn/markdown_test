journal = {Science},
keywords = {Agriculture,Animals,Biodiversity,Ecosystem,Extinction, Biological,Food Chain,Great Britain,Insects,Insects: physiology,Models, Biological,Parasites,Parasites: physiology,Plant Physiological Phenomena,Pollination,Vertebrates,Vertebrates: physiology},
month = feb,
number = {6071},
pages = {973--7},
pmid = {22363009},
title = {{The robustness and restoration of a network of ecological networks.}},
