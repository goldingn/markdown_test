journal = {Journal of medical entomology},
keywords = {Animals,Anopheles,Anopheles gambiae,Anopheles gambiae: genetics,Anopheles gambiae: parasitology,Anopheles gambiae: physiology,Anopheles: physiology,Body Size,Body Size: physiology,Female,Fertility,Fertility: physiology,Ovum,Ovum: chemistry,Ovum: physiology,Plasmodium falciparum,Plasmodium falciparum: isolation \& purification,Proteins,Proteins: analysis,Species Specificity},
month = sep,
number = {5},
