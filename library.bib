journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {Animals,Anopheles gambiae,Anopheles gambiae: physiology,Body Size,Female,Population Density,Population Dynamics,Rain,Tanzania},
month = oct,
number = {1721},
pages = {3142--51},
pmid = {21389034},
title = {{Linking individual phenotype to density-dependent population growth: the influence of body size on the population dynamics of malaria vectors.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3158942\&tool=pmcentrez\&rendertype=abstract},
