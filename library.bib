keywords = {avaritia,bluetongue virus,breeding habitat,culicoides,development,larval,parity,pigmentation,vectors},
month = mar,
pmid = {23458570},
title = {{Larval development and emergence sites of farm-associated Culicoides in the United Kingdom}},
