issn = {03043800},
journal = {Ecological Modelling},
number = {3-4},
pages = {324--332},
title = {{A raster-based technique for analysing habitat configuration: The cost–benefit approach}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0304380006005291},
