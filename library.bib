@misc{Alves2009,
abstract = {Avian malaria is a globally distributed vector-borne disease, caused by Plasmodium and Haemoproteus protozoan parasites and transmitted by blood sucking Diptera (Valkiūnas, 2005). Symptoms may include declines in food consumption and activity levels to severe anaemia, capable of leading to high mortality, particularly in the case of island endemic bird populations (Atkinson et al., 2000; Woodworth et al., 2004).  Recent developments in molecular diagnosis techniques have made avian malaria an excellent system for the study of the ecology and evolution of host-vector-parasite interactions. 	Avian malaria is endemic in the long-term tits study populations at Wytham Woods, near Oxford, where approximately 30\% of breeding adult blue tits Cyanistes caeruleus are infected with avian malaria (Wood et al., 2007; Cosgrove et al., 2008). The host-parasite system has been well studied, showing the presence of diverse parasite lineages with marked spatiotemporal variation in prevalence (Wood et al., 2007; Cosgrove et al., 2008), while the ecology of the hosts is well known. However, this system lacks crucial knowledge regarding the vectors of avian malaria, which remains almost unstudied.},
author = {Alves, R and Wood, MJ and Knowles, SCL and Sheldon, BC},
file = {:C$\backslash$:/Users/Nick/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Alves et al. - 2009 - Vector ecology of avian malaria in tits (Paridae)(2).pdf:pdf},
