journal = {The American journal of tropical medicine and hygiene},
keywords = {Aedes,Aedes: genetics,Aedes: immunology,Aedes: virology,Animals,Chikungunya virus,Chikungunya virus: immunology,Chikungunya virus: physiology,Defensins,Defensins: genetics,Defensins: immunology,Female,HSP70 Heat-Shock Proteins,HSP70 Heat-Shock Proteins: genetics,HSP70 Heat-Shock Proteins: immunology,Heat Stress Disorders,Heat Stress Disorders: immunology,Heat Stress Disorders: virology,Hot Temperature,Lectins,Lectins: genetics,Lectins: immunology,Nitric Oxide Synthase,Nitric Oxide Synthase: genetics,Nitric Oxide Synthase: immunology,Polymerase Chain Reaction,RNA, Viral,RNA, Viral: chemistry,RNA, Viral: genetics,Serine Endopeptidases,Serine Endopeptidases: genetics,Serine Endopeptidases: immunology},
