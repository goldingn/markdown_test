keywords = {Anopheles,Larvae,Malaria,Remote sensing,SPOT},
mendeley-tags = {Anopheles,Larvae,Malaria,Remote sensing,SPOT},
number = {5},
pages = {482--488},
title = {{Predictions of adult Anopheles albimanus densities in villages based on distances to remotely sensed larval habitats}},
