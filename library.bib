journal = {International Journal of Health Geographics},
keywords = {Animals,Anopheles,Culicidae,Ecosystem,Environmental Monitoring,Environmental Monitoring: methods,Geographic Information Systems,Humans,Insect Vectors,Larva,Malaria,Malaria: epidemiology,Malaria: prevention \& control,Malaria: transmission,Mosquito Control,Mosquito Control: methods,Tanzania,Tanzania: epidemiology,Topography, Medical,Topography, Medical: methods,Urban Population},
