issn = {1932-6203},
journal = {PLoS ONE},
month = jan,
number = {3},
pages = {e9927},
pmid = {20360950},
title = {{Environmental factors associated with the distribution of Anopheles gambiae s.s in Ghana; an important vector of lymphatic filariasis and malaria.}},
