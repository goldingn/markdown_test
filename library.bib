issn = {0269-283X},
journal = {Medical and Veterinary Entomology},
keywords = {Animals,Anopheles,Anopheles: classification,Anopheles: drug effects,Anopheles: physiology,Demography,Drug Evaluation,Geography,Ghana,Humans,Insect Vectors,Insecticides,Insecticides: pharmacology,Malaria,Malaria: transmission,Nigeria,Preclinical,Pyrethrins,Pyrethrins: pharmacology,Rain,Rural Health,Seasons,Urban Health},
