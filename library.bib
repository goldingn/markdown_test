journal = {Acta Tropica},
keywords = {Aedes,Aedes: physiology,Aedes: virology,Alphavirus Infections,Alphavirus Infections: transmission,Alphavirus Infections: virology,Animals,Chikungunya virus,France,Humans,Insect Vectors,Insect Vectors: physiology,Insect Vectors: virology,Italy},
