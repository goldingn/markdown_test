month = jan,
pages = {149},
pmid = {19580663},
title = {{Comparative evaluation of four mosquitoes sampling methods in rice irrigation schemes of lower Moshi, northern Tanzania.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2712478\&tool=pmcentrez\&rendertype=abstract},
