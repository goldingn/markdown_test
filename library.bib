@article{Hutchinson2006a,
abstract = {There is convincing evidence that between the 16th and 19th centuries, individuals living in the coastal marshes of southern England were less healthy than those in inland areas.[1], [2], [3], [4] and [5] This evidence comes from two main sources. Historical records such as diaries, surveys, and travellers' accounts, provide anecdotal information about conditions in the marshes,[6] and [7] and Anglican parish registers of burials and baptisms provide data from which demographic trends can be identified.8 The insalubrious marshes were associated with a regularly occurring fever, known as ague, marsh fever, tertian fever, and intermittent fever.9 This ague is most likely to have been malaria because it was often characterised by typical symptoms of this infection, including splenomegaly and fevers every 48 h.[10] and 11 M Hunter and A Gregory, An astrological diary of the seventeenth century. Samuel Jeake of Rye 1652–1699, Clarendon Press, Oxford (1988).[11] Most importantly, this fever of the marshes responded to treatment with quinine,[12] and [13] which is an effective antimalarial. The malaria parasite was probably Plasmodium vivax, rather than P falciparum, since vivax malaria always dominates in temperate climates10 and the UK climate is theoretically capable of supporting vivax transmission.14 More compelling evidence comes from the identification of P vivax in bloodfilms obtained in 1917 from patients with the same symptoms as those described more than a century earlier.15 We also know from feeding experiments with the mosquito Anopheles atroparvus—the only common vector species in the saltmarshes—that these mosquitoes could transmit vivax, but not falciparum malaria.[16] and [17] 
Mary Dobson,2 in her comprehensive historical analysis, concluded that the high mortality rate in the marshes was due in small part to the direct effects of vivax malaria and speculated that, unlike other vivax strains, English vivax might have been a killer. She argued that vivax was an important indirect cause of death in individuals with little immunity against malaria who were malnourished and sick with other potentially life-threatening infections. These conclusions have been overinterpreted by the scientific community, including ourselves, who claim that malaria was the killer in the marshes. Thus recent articles assessing the future risk of malaria in the UK begin by describing the disease as having been a major cause of death in the past.[14], [18], [19], [20], [21], [22], [23] and [24] The almost inevitable result of this conclusion is to raise concerns about whether we will return to this state as our environment changes. Of even greater concern is that historical records of ague deaths have been used in complex statistical analyses of how environmental change affected malaria in the past,22 or to examine how past climate affected malaria deaths;[18], [19] and [22] historical analyses are now being used to provide insights into how environmental changes will affect malaria transmission in the future. Here we challenge the overly simplistic view that English malaria was the cause of the high mortality in the marshes, and suggest that these high rates were due to other causes. },
annote = {ISI Document Delivery No.: 051XG
Times Cited: 3
Cited Reference Count: 58},
author = {Hutchinson, Robert A and Lindsay, Steve W},
file = {:C$\backslash$:/Users/Nick/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hutchinson, Lindsay - 2006 - Malaria and deaths in the English marshes(2).pdf:pdf},
isbn = {0140-6736},
journal = {Lancet},
keywords = {AGUE,ENGLAND,FUTURE,HISTORY,Historical,MATERNAL MORTALITY,MOZAMBIQUE,Malaria,PLASMODIUM-VIVAX,UK},
mendeley-tags = {Historical,Malaria,UK},
number = {9526},
pages = {1947--1951},
title = {{Malaria and deaths in the English marshes}},
url = {<Go to ISI>://000238194400034  file:///E:/Documents and Settings/Administrator/Desktop/Papers/Introduced disease risk/Hutchinson 2006.pdf},
volume = {367},
year = {2006}
}
@article{Gavin2003,
abstract = {This paper describes the application of a simple model to investigate the groundwater movement of a wet grassland in Southeast England. The wetland is subject to an agri-environmental scheme, which specifies the manipulation of ditch water levels to promote high water tables for ecological purposes. Field data from the North Kent Marshes are input to the model Drain Interaction with Channel Hydrology (DITCH) to simulate groundwater change and to quantify recharge from a ditch to the adjacent land parcel. The area is covered with an alluvial clay soil, and a lowvalue of hydraulic conductivitywas computed using the Guelph permeameter. The seasonal change of groundwater occurred at faster rates than the hydraulic conductivity suggesting macropore flow. In DITCH, specific yield is used as a measure of macropore flow. Sensitivity analysis of specific yield is subject to a six-step evaluation and shows that a value of 0.15 is optimal, more characteristic of a silt than a clay soil. The model output shows non-coupling between the ditch water level and groundwater in the adjacent parcel of land, with negligible recharge from the ditch; despite the maintenance of high ditch water levels, the dominant hydrological fluxes are vertical. It is concluded that the standard management practise of maintaining high ditch water levels to encourage lateral water movement through the soil matrix is not effective in this wet grassland.},
author = {Gavin, H.},
doi = {10.1016/S0167-8809(03)00201-9},
file = {:C$\backslash$:/Users/Nick/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gavin - 2003 - Impact of ditch management on the water levels of a wet grassland in Southeast England(2).pdf:pdf},
issn = {01678809},
journal = {Agriculture, Ecosystems \& Environment},
keywords = {environmental management,hydrology,modelling,wet grassland},
month = oct,
number = {1-3},
pages = {51--60},
title = {{Impact of ditch management on the water levels of a wet grassland in Southeast England}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0167880903002019},
volume = {99},
year = {2003}
}
@article{Carpenter2009,
abstract = {In June 2006, bluetongue virus, an arboviral pathogen of ruminants, appeared in northern Europe for the first time, successfully overwintered and subsequently caused substantial losses to the farming sector in 2007 and 2008. This emergence served as a test of how the probability of arboviral incursion into new regions is assessed and has highlighted the reliance of decision making on paradigms that are not always underpinned by basic biological data. In this review, we highlight those areas of the epidemiology of bluetongue that are poorly understood, reflect upon why certain vital areas of research have received little attention and, finally, examine strategies that could aid future risk assessment and intervention.},
