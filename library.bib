keywords = {Culex,Mosquito ecology,UK,UK Species},
mendeley-tags = {Culex,Mosquito ecology,UK,UK Species},
pages = {99--111},
title = {{Population studies, physiological state and mortality factors of overwintering adult populations of females of Culex pipiens L. (Diptera: Culicidae)}},
volume = {77},
year = {1987}
}
