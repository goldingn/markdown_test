journal = {Bulletin of Entomological Research},
keywords = {Animals,CDC trap,Carbon Dioxide,Culicidae,Environmental Monitoring,Environmental Monitoring: methods,Female,Great Britain,Insect Vectors,Light,Mosquito Control,Mosquito Control: methods,Mosquito magnet,Pilot Projects,UK},
