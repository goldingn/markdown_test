issn = {1932-6203},
journal = {PLoS ONE},
keywords = {Animals,Biodiversity,Disease Reservoirs,Disease Vectors,Ecosystem,Geography,Leishmania,Leishmaniasis,Leishmaniasis: metabolism,Mexico},
month = jan,
number = {5},
