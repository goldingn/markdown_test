file = {:C$\backslash$:/Users/Nick/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Tachiiri et al. - 2006 - Predicting outbreaks a spatial risk assessment of West Nile virus in British Columbia.pdf:pdf},
