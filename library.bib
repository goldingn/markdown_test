isbn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {biodiversity,borrelia-burgdorferi,forest fragments,habitat patches,ixodes-scapularis acari,ixodidae,mice,peromyscus-leucopus,populations,spirochete},
number = {2},
pages = {567--571},
title = {{The ecology of infectious disease: Effects of host diversity and community composition on Lyme disease risk}},
