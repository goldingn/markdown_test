journal = {Malaria Journal},
keywords = {19th Century,Animals,Anopheles,Anopheles: classification,Anopheles: growth \& development,Anopheles: parasitology,Death Certificates,Disease Outbreaks,Disease Outbreaks: history,Endemic Diseases,Endemic Diseases: history,Finland,Finland: epidemiology,History,Humans,Insect Vectors,Insect Vectors: classification,Insect Vectors: growth \& development,Insect Vectors: parasitology,Malaria,Plasmodium vivax,Plasmodium vivax: isolation \& purification,Seasons,Temperature,Vivax,Vivax: epidemiology,Vivax: history,Vivax: mortality},
