month = may,
number = {5},
pages = {e75},
pmid = {17511518},
title = {{Out of Africa: a molecular perspective on the introduction of yellow fever virus into the Americas.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1868956\&tool=pmcentrez\&rendertype=abstract},
