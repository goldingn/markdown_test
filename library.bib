@article{LeMenach2005,
abstract = {BACKGROUND: Mosquitoes commute between blood-meal hosts and water. Thus, heterogeneity in human biting reflects underlying spatial heterogeneity in the distribution and suitability of larval habitat as well as inherent differences in the attractiveness, suitability and distribution of blood-meal hosts. One of the possible strategies of malaria control is to identify local vector species and then attack water bodies that contain their larvae. METHODS: Biting and host seeking, not oviposition, have been the focus of most previous studies of mosquitoes and malaria transmission. This study presents a mathematical model that incorporates mosquito oviposition behaviour. RESULTS: The model demonstrates that oviposition is one potential factor explaining heterogeneous biting and vector distribution in a landscape with a heterogeneous distribution of larval habitat. Adult female mosquitoes tend to aggregate around places where they oviposit, thereby increasing the risk of malaria, regardless of the suitability of the habitat for larval development. Thus, a water body may be unsuitable for adult mosquito emergence, but simultaneously, be a source for human malaria. CONCLUSION: Larval density may be a misleading indicator of a habitat's importance for malaria control. Even if mosquitoes could be lured to oviposit in sprayed larval habitats, this would not necessarily mitigate -- and might aggravate -- the risk of malaria transmission. Forcing mosquitoes to fly away from humans in search of larval habitat may be a more efficient way to reduce the risk of malaria than killing larvae. Thus, draining, fouling, or filling standing water where mosquitoes oviposit can be more effective than applying larvicide.},
author = {{Le Menach}, Arnaud and McKenzie, F Ellis and Flahault, Antoine and Smith, David L.},
doi = {10.1186/1475-2875-4-23},
file = {:C$\backslash$:/Users/Nick/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Le Menach et al. - 2005 - The unexpected importance of mosquito oviposition behaviour for malaria non-productive larval habitats can be sources for malaria transmission.pdf:pdf},
issn = {1475-2875},
journal = {Malaria Journal},
keywords = {Animal,Animal: physiology,Animals,Biological,Breeding,Culicidae,Culicidae: physiology,Ecosystem,Falciparum,Falciparum: epidemiology,Falciparum: parasitology,Falciparum: transmission,Fresh Water,Humans,Insect Vectors,Insect Vectors: physiology,Larva,Larva: physiology,Malaria,Models,Oviposition,Oviposition: physiology,Plasmodium falciparum,Plasmodium falciparum: physiology,Prevalence,Risk Factors,Sexual Behavior},
month = jan,
number = {1},
pages = {23},
pmid = {15892886},
title = {{The unexpected importance of mosquito oviposition behaviour for malaria: non-productive larval habitats can be sources for malaria transmission.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15892886},
volume = {4},
year = {2005}
}
@article{Service1994,
abstract = {Culex torrentium Martini was first recorded as a British mosquito by Mattingly (1951) but, by checking specimens in museum collections, Service (1968) found that the species was present in Britain from at least 1900. Morphologically Cx torrentium is very similar to Culexpipiens L., adults of the former usually having more pre-alar scales than the latter species (Service, 1968; Jupp, 1979; Onyeka, 1982). The terminalia of male Cx torrentium are quite distinct from those of Cx pipiens (Mattingly, 1951; Service, 1968). Females of both species are principally ornithophagic (Service, 1969); larvae of both are found in various manmade containers and in ground waters, often together (Service, 1968; Jupp, 1979; Gillies \& Gubbins, 1982). Onyeka (1980) and Bradshaw \& Holzapfel(l991) collected larvae of Cx torrentium from tree-holes in Britain, but I have been unable to find them in this type of habitat. Jupp (1979), Service (1968), Onyeka (1980) and Sulairnan \& Service (1983) found no evidence of female adults in hibernation sites used by Cx pipiens. Gillies \& Gubbins (1982) found that a few females in hibernation sites used by Cx pipiens had pre-alar scales - but concluded that, because the presence or absence of these scales was not an absolutely diagnostic character for separating the two species, they could not exclude the possibility that a few of the hibernating adult females were Cx torrentiurn. Nevertheless, they concluded that the principal hibernation sites of Cx torrentium were not the same as those of Cx pipiens. In Sweden, however, Jaenson (1987) collected hibernating adult females of both Cx torrentium and Cxpipiens from a cellar},
